export PATH=$HOME/bin:$PATH

# Bin Scripts
alias gc="ref-count"

# Unix aliases
alias cl="clear"
alias eba="vim ~/.bash_aliases"

# Axial aliases
alias axms="sudo svstat /service/* | column -t"
alias axmg="node node_modules/gulp/bin/gulp.js"
alias axml='tail -F /var/log/axm/events.log | sed "s/#012/\n/g"'

